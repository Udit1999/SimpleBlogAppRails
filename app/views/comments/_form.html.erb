<%= form_with model: [ @article, @article.comments.build ] do |form| %>
  <div class="field">
    <%= form.label :commenter %>
    <%= form.text_field :commenter %>
  </div>

  <div class="field">
    <%= form.label :body %>
    <%= form.text_area :body %>
  </div>

  <div class="field">
    <%= form.label :article_id %>
    <%= form.text_field :article_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
